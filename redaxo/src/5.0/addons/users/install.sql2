CREATE TABLE `rex_user_role` ( `id` int(11) NOT NULL auto_increment, `name` varchar(255) , `description` text ,  `rights` text NOT NULL  , `createuser` varchar(255) NOT NULL  , `updateuser` varchar(255) NOT NULL  , `createdate` int(11) NOT NULL , `updatedate` int(11) NOT NULL , `revision` int(11) NOT NULL, PRIMARY KEY(`id`))ENGINE=MyISAM;
